{
  "name": "Grupo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "juego": {
      "type": "belongsTo",
      "model": "Juego",
      "foreignKey": ""
    },
    "pruebas": {
      "type": "hasMany",
      "model": "Prueba",
      "foreignKey": "",
      "through": "Marcador"
    },
    "miembros": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "",
      "through": "Miembro"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}

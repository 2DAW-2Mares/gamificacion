{
  "name": "Juego",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string"
    },
    "imagen": {
      "type": "string"
    },
    "grupal": {
      "type": "boolean"
    }
  },
  "validations": [],
  "relations": {
    "coordinadores": {
      "type": "hasAndBelongsToMany",
      "model": "Usuario",
      "foreignKey": ""
    },
    "creador": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "creador"
    },
    "pruebas": {
      "type": "hasMany",
      "model": "Prueba",
      "foreignKey": ""
    },
    "grupos": {
      "type": "hasMany",
      "model": "Grupo",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
